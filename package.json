{
	"name": "some-random-api",
	"version": "1.2.0",
	"description": "some random apis for all your needs",
	"main": "dist/index.js",
	"types": "typings/index.d.ts",
	"scripts": {
		"test": "node test",
		"watch": "tsc -w",
		"postinstall": "npm run build",
		"build": "tsc",
		"build:watch": "tsc -w",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"prepare": "npm run build",
		"prepublishOnly": "npm run lint",
		"preversion": "npm run lint",
		"version": "git add -A src",
		"postversion": "git push origin master && git push origin master --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/telkenes/some-random-api.git"
	},
	"keywords": [
		"api",
		"free"
	],
	"author": "Telk",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/telkenes/some-random-api/issues"
	},
	"homepage": "https://github.com/telkenes/some-random-api#readme",
	"dependencies": {
		"node-fetch": "^2.2.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"eslint": "^6.7.1",
		"eslint-config-marine": "^5.3.2",
		"@types/node": "^12.12.14",
		"@types/node-fetch": "^2.5.4",
		"typescript": "^3.7.2"
	},
	"eslintConfig": {
		"extends": "marine/node",
		"rules": {
			"@typescript-eslint/explicit-member-accessibility": "off"
		}
	}
}
